{
  "DataProcessor": {
    "name": "DataProcessor",
    "connector": "memory"
  },
  "adp": {
    "name": "adp",
    "baseURL": "http://localhost:65026",
    "crud": false,
    "connector": "rest",
    "debug": false,
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json",
        "authorization": "Bearer {accessToken}"
      }
    },
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "User",
          "query": {
            "subscriberId": "{subscriberId}"
          },
          "headers": {
            "authorization": "Bearer {accessToken}"
          }          
        },
        "functions": {
          "getUserBySubscriberId": [
            "accessToken",
            "subscriberId"
          ]
        }
      }
    ]
  },
  "AuthToken": {
    "name": "AuthToken",
    "crud": false,
    "connector": "rest",
    "debug": false,
    "options": {
      "headers": {
        "accept": "application/json",
        "content-type": "application/json"
      }
    },
    "operations": [
      {
        "template": {
          "method": "POST",
          "url": "https://jestercreditunion-dvlp.auth0.com/oauth/token",
          "body": {
            "client_id": "{clientId}",
            "client_secret": "{clientSecret}",
            "audience": "{audience}",
            "grant_type": "client_credentials"
          }
        },
        "functions": {
          "getToken": [
            "clientId",
            "clientSecret",
            "audience"
          ]
        }
      }
    ]
  }
}
